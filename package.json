{
  "name": "antigravity-plus",
  "displayName": "%extension.displayName%",
  "description": "%extension.description%",
  "version": "0.0.14",
  "publisher": "ImL1s",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other",
    "Machine Learning"
  ],
  "keywords": [
    "antigravity",
    "ai",
    "agent",
    "auto-approve",
    "quota",
    "monitor",
    "gemini"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "antigravity-plus.openDashboard",
        "title": "%command.openDashboard%",
        "category": "Antigravity Plus"
      },
      {
        "command": "antigravity-plus.toggleAutoApprove",
        "title": "%command.toggleAutoApprove%",
        "category": "Antigravity Plus"
      },
      {
        "command": "antigravity-plus.refreshQuota",
        "title": "%command.refreshQuota%",
        "category": "Antigravity Plus"
      },
      {
        "command": "antigravity-plus.resetSession",
        "title": "%command.resetSession%",
        "category": "Antigravity Plus"
      },
      {
        "command": "antigravity-plus.showLogs",
        "title": "%command.showLogs%",
        "category": "Antigravity Plus"
      },
      {
        "command": "antigravity-plus.showQuickPick",
        "title": "%command.showQuickPick%",
        "category": "Antigravity Plus"
      },
      {
        "command": "antigravity-plus.toggleAutoWakeup",
        "title": "%command.toggleAutoWakeup%",
        "category": "Antigravity Plus"
      },
      {
        "command": "antigravity-plus.optimizeContext",
        "title": "%command.optimizeContext%",
        "category": "Antigravity Plus"
      }
    ],
    "configuration": {
      "title": "%config.title%",
      "properties": {
        "antigravity-plus.autoApprove.fileOperations": {
          "type": "boolean",
          "default": true,
          "description": "%config.autoApprove.fileOperations.description%"
        },
        "antigravity-plus.autoApprove.terminalCommands": {
          "type": "boolean",
          "default": true,
          "description": "%config.autoApprove.terminalCommands.description%"
        },
        "antigravity-plus.autoApprove.denyList": {
          "type": "array",
          "default": [
            "rm -rf /",
            "rm -rf ~",
            "rm -rf /*",
            "format c:",
            "format d:",
            "del /f /s /q c:\\*",
            "dd if=/dev/zero",
            "mkfs",
            ":(){:|:&};:",
            "chmod -R 777 /",
            "shutdown",
            "reboot"
          ],
          "description": "%config.autoApprove.denyList.description%"
        },
        "antigravity-plus.autoApprove.allowList": {
          "type": "array",
          "default": [],
          "description": "%config.autoApprove.allowList.description%"
        },
        "antigravity-plus.autoApprove.interval": {
          "type": "number",
          "default": 200,
          "minimum": 100,
          "description": "%config.autoApprove.interval.description%"
        },
        "antigravity-plus.autoApprove.enabled": {
          "type": "boolean",
          "default": false,
          "description": "%config.autoApprove.enabled.description%"
        },
        "antigravity-plus.quotaMonitor.enabled": {
          "type": "boolean",
          "default": true,
          "description": "%config.quotaMonitor.enabled.description%"
        },
        "antigravity-plus.quotaMonitor.pollingInterval": {
          "type": "number",
          "default": 60,
          "minimum": 10,
          "maximum": 600,
          "description": "%config.quotaMonitor.pollingInterval.description%"
        },
        "antigravity-plus.quotaMonitor.warningThreshold": {
          "type": "number",
          "default": 50,
          "minimum": 0,
          "maximum": 100,
          "description": "%config.quotaMonitor.warningThreshold.description%"
        },
        "antigravity-plus.quotaMonitor.criticalThreshold": {
          "type": "number",
          "default": 30,
          "minimum": 0,
          "maximum": 100,
          "description": "%config.quotaMonitor.criticalThreshold.description%"
        },
        "antigravity-plus.quotaMonitor.displayStyle": {
          "type": "string",
          "default": "percentage",
          "enum": [
            "progressBar",
            "percentage",
            "dots"
          ],
          "enumDescriptions": [
            "%config.quotaMonitor.displayStyle.enumDescriptions.progressBar%",
            "%config.quotaMonitor.displayStyle.enumDescriptions.percentage%",
            "%config.quotaMonitor.displayStyle.enumDescriptions.dots%"
          ],
          "description": "%config.quotaMonitor.displayStyle.description%"
        },
        "antigravity-plus.quotaMonitor.showGeminiPro": {
          "type": "boolean",
          "default": true,
          "description": "%config.quotaMonitor.showGeminiPro.description%"
        },
        "antigravity-plus.quotaMonitor.showGeminiFlash": {
          "type": "boolean",
          "default": true,
          "description": "%config.quotaMonitor.showGeminiFlash.description%"
        },
        "antigravity-plus.quotaMonitor.showCostEstimate": {
          "type": "boolean",
          "default": true,
          "description": "%config.quotaMonitor.showCostEstimate.description%"
        },
        "antigravity-plus.ui.language": {
          "type": "string",
          "default": "auto",
          "enum": [
            "auto",
            "en",
            "zh-tw",
            "zh-cn",
            "ja",
            "ko",
            "es",
            "fr",
            "de",
            "it",
            "pt-br"
          ],
          "enumDescriptions": [
            "自動偵測 (Auto)",
            "English",
            "繁體中文",
            "简体中文",
            "日本語",
            "한국어",
            "Español",
            "Français",
            "Deutsch",
            "Italiano",
            "Português (Brasil)"
          ],
          "description": "%config.ui.language.description%"
        },
        "antigravity-plus.wakeupConfig.enabled": {
          "type": "boolean",
          "default": false,
          "description": "Enable Smart Auto Wake-up (自動喚醒)"
        },
        "antigravity-plus.wakeupConfig.mode": {
          "type": "string",
          "enum": [
            "smart",
            "fixed",
            "cron"
          ],
          "default": "smart",
          "description": "Scheduling Mode (排程模式)"
        },
        "antigravity-plus.wakeupConfig.workStartTime": {
          "type": "string",
          "default": "09:00",
          "description": "Work Start Time (工作開始時間, Smart 模式會提早 3 小時喚醒)"
        },
        "antigravity-plus.wakeupConfig.notificationWebhook": {
          "type": "string",
          "default": "",
          "description": "Webhook URL for Notifications (Discord/Slack/Teams)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "npm run compile && vscode-test",
    "test:unit": "npm run compile && mocha --ui bdd --timeout 15000 --require out/test/unit/setup.js out/test/unit/**/*.test.js",
    "test:smoke": "npm run compile && node out/test/smoke/runTest.js",
    "test:e2e": "npm run compile && node out/test/e2e/runTest.js",
    "test:integration": "npm run compile && node out/test/integration/runTest.js",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:smoke && npm run test:e2e",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "@types/vscode": "^1.85.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "@vscode/vsce": "^2.32.0",
    "eslint": "^8.55.0",
    "glob": "^13.0.0",
    "mocha": "^11.7.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "uuid": "^9.0.0",
    "ws": "^8.18.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ImL1s/antigravity-plus"
  },
  "license": "MIT",
  "icon": "resources/icons/icon.png"
}